<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="Location" table="LOCATION">
  <composite-id>
   <key-property name="noClient" type="int">
    <column name="NOCLIENT" />
   </key-property>
   <key-property name="noFilm" type="int">
    <column name="NOFILM" />
   </key-property>
  </composite-id>
  <property name="dateLocation" column="date">
   <column name="DATELOCATION" />
  </property>
  <property name="dateRetour" type="date">
   <column name="DATERETOUR" />
  </property>
 </class>
</hibernate-mapping>