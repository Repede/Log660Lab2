<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="Film" table="FILM">
	<id name="noFilm" type="int">
		<column name="NOFILM"/>
		<generator class="native"/>
	</id>
	<property name="titre" column="TITRE"/>
	<property name="langue" column="LANGUE"/>
	<property name="duree" column="DUREE"/>
	<property name="annee" column="ANNEE"/>
	<property name="sypnosis" column="SYPNOSIS"/>
	
 </class>
</hibernate-mapping>