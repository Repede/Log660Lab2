<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="Film" table="FILM">
  <id name="noFilm" type="int">
   <column name="NOFILM"/>
   <generator class="native"/>
  </id>
  <property name="titre" type="string">
   <column name="TITRE" />
  </property>
  <property name="langue" type="string">
   <column name="LANGUE" not-null="true" />
  </property>
  <property name="duree" type="int">
   <column name="DUREE" />
  </property>
  <property name="annee" type="int">
   <column name="ANNEE" />
  </property>
  <property name="sypnosis" type="string">
   <column name="SYPNOSIS" />
  </property>
 </class>
</hibernate-mapping>